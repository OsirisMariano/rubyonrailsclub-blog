<!DOCTYPE html>
<html>
  <head>
    <title>RubyonrailsclubBlog</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= render "layouts/shared/aplication/header" %>
    <%= render "layouts/shared/aplication/main_banner" %>
    <%= render "layouts/shared/aplication/articles" %>
    <%= render "layouts/shared/aplication/categories" %>
    <%= render "layouts/shared/aplication/footer" %>
    
    <%= yield %>

    <%= javascript_include_tag "menu", "data-turbo-track": "reload" %>
    <%= javascript_include_tag "posts-grid", "data-turbo-track": "reload" %>
    <%= javascript_include_tag "jquery/jquery1.min", "data-turbo-track": "reload" %>
    <%= javascript_include_tag "slick/slick.min", "data-turbo-track": "reload" %>
  </body>
</html>
